<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Questions Data</title>
  </head>
  <body>
    <h1>Questions Data</h1>
    <table border="1" width="100%">
      <thead>
        <tr>
          <th>Question ID</th>
          <th>Question</th>
          <th>Options</th>
          <th>Answer Type</th>
          <th>Yes Question Index</th>
          <th>No Question Index</th>
          <th>Category Type</th>
        </tr>
      </thead>
      <tbody>
        {{#each data}}
        {{#each this.questions}}
        <tr>
          <td>{{this.question_id}}</td>
          <td>{{this.question}}</td>
          <td>{{this.options}}</td>
          <td>{{this.Answer_type}}</td>
          <td>{{this.yesQuestionIndex}}</td>
          <td>{{this.noQuestionIndex}}</td>
          <td>{{this.category_type}}</td>
        </tr>
        {{/each}}
        {{/each}}
      </tbody>
    </table>
  </body>
</html>
